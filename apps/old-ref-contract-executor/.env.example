# ============================================
# 1. SERVER CONFIG (2 variables)
# ============================================
PORT=3001
NODE_ENV=development

# ============================================
# 2. CHAIN SELECTION (1 variable)
# ============================================
DEFAULT_CHAIN_ID=11155111    # Sepolia chain ID

# ============================================
# 3. RPC CONNECTION (1 variable)
# ============================================
RPC_URL_SEPOLIA=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY
# Or use Infura:
# RPC_URL_SEPOLIA=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
# Or public RPC (slower):
# RPC_URL_SEPOLIA=https://rpc.sepolia.org

# ============================================
# 4. WALLET SIGNERS (2 variables - REQUIRED)
# ============================================
SAFE_SIGNER_PRIVATE_KEY_1=0xYOUR_ORACLE_PRIVATE_KEY_HERE
SAFE_SIGNER_PRIVATE_KEY_2=0xYOUR_GUARDIAN_PRIVATE_KEY_HERE

# ============================================
# 5. GNOSIS SAFE MULTISIG (1 variable)
# ============================================
SAFE_ADDRESS_SEPOLIA=0xYOUR_GNOSIS_SAFE_ADDRESS_ON_SEPOLIA

# ============================================
# 6. PROXIFY CONTRACT ADDRESSES (3 variables)
# ============================================
PROXIFY_ADDRESS_SEPOLIA=0xYOUR_PROXIFY_CONTRACT_ADDRESS
PROXIFY_CONTROLLER_ADDRESS_SEPOLIA=0xYOUR_PROXIFY_CONTROLLER_ADDRESS
PROXIFY_CLIENT_REGISTRY_ADDRESS_SEPOLIA=0xYOUR_PROXIFY_CLIENT_REGISTRY_ADDRESS