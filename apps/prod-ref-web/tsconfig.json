{
	"extends": "tsconfig/vite.json",
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"module": "ESNext",
		"rootDirs": ["src", "vite.config.ts"],
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist", "**/*.config.ts", "**/*.config.mjs", "**/*.config.js"]
}
