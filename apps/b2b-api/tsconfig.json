{
	"extends": "@proxify/tsconfig/base.json",
	"compilerOptions": {
		"outDir": "./dist",
		"rootDir": "./src",
		"baseUrl": "./src",
		"paths": {
			"@/*": ["./*"]
		},
		"skipLibCheck": true,
		"declaration": false,
		"declarationMap": false
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}
