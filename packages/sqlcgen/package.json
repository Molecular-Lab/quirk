{
	"name": "@quirk/sqlcgen",
	"version": "1.0.0",
	"description": "SQLC-generated TypeScript queries for Quirk B2B platform",
	"private": true,
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"default": "./src/index.ts"
		},
		"./src/gen/*": {
			"types": "./src/gen/*.ts",
			"default": "./src/gen/*.ts"
		}
	},
	"scripts": {
		"generate": "sqlc generate",
		"migrate": "../scripts/migrate.sh"
	},
	"dependencies": {
		"postgres": "^3.4.7"
	},
	"keywords": [
		"sqlc",
		"postgres",
		"database",
		"b2b"
	],
	"author": "",
	"license": "ISC"
}