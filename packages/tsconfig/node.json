{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node.js",
    "extends": "./base.json",
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "lib": [
            "ES2020"
        ],
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "types": [
            "node"
        ]
    }
}