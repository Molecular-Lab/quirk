# Server Config
PORT=3001
NODE_ENV=development

# Blockchain Config - Multi-chain Support
DEFAULT_CHAIN_ID=11155111

# RPC URLs (Add your Alchemy/Infura keys)
RPC_URL_SEPOLIA=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY
